#include <stdio.h>
 
int main(){
    int n,i,j,maior,aux;
    int vt_ord[100];

    scanf("%d", &n);

    for(i=0;i<n;i++){
        scanf("%d", &vt_ord[i]);
    }

    for(i=n-1;i==1;i--){
        maior=0;
        printf("oi\n");
        for(j=0;j==i;j++){
            if ((vt_ord[j])>(vt_ord[maior])){
                maior=j;
            }
        }
        aux=vt_ord[i];
        vt_ord[i]=vt_ord[maior];
        vt_ord[maior]=aux;
    }

    for(i=0;i<n;i++){
        printf("%d ", vt_ord[i]);
    }
    printf("\n");

    return 0;
}
