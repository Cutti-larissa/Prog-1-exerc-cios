#include <stdio.h>

int main(){
    
    int n, v1, v2, v3, confere;
    
    scanf("%d", &n);
    
    v1 = 1;
    confere = 0;
    
    while ((v1 != n/2)&(!confere)){
        v2 = v1+1;
        v3 = v2+1;
        if (v1*v2*v3==n)
            confere = 1;
        v1++;
    }
    
    printf("%d\n",confere);
    
}
